<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='com.ufpor.app.client.view.project.FullPopUpView.FullPopUpStyle'>
	.main {
		width: 100%;
		height: 100%;
		padding: .5em;
	}

    .mainOverlay {
        width: 100%;
        height: 100%;
        padding: .5em;
        position:relative;

    }
	
	.leftPanel,.rightPanel {
		width: 44%;
		padding: 1em;
		height: 80%;
		float: left;
		border: 1px solid black;
	}
	
	.leftPanel {
		margin-right: 1em;
	}
	
	.rightPanel {
		
	}
	
	.singleRow {
		width: 100%;
		height: 17%;
	}


	
	.textBoxContainer {
		float: left;
		height: 90%;
		width: 50%;
	}
	
	.labelContainer {
		float: left;
		height: 100%;
		width: 40%;
	}
	
	.suffixContainer {
		float: left;
		height: 100%;
		width: 10%;
        padding-left: -10px;
	}
	
	.label {
		float: left;
		position: relative;
		top: 50%;
		transform: translateY(-50%);
	}
	
	.suffix {
		float: left;
		position: relative;
		top: 50%;
		transform: translateY(-50%);
		width: 30%;
	}
	
	.textBox {
		padding: 0px;
		position: relative;
		float: left;
		top: 50%;
		transform: translateY(-50%);
		width: 80%;
		height: 18px;
	}
	
	.bigRow {
		width: 90%;
		height: 35%;
	}
	
	.bigTextBox {
		padding: 0px;
		width: 90%;
		height: 90%;
	}
	</ui:style>
