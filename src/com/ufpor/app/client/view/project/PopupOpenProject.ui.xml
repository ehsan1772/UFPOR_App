<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.main {
		width: 100%;
		height: 100%;
	}
	
	.top {
		width: 100%;
		height: 20%;
		background-color: #A399C2;
	}
	
	.middle {
		width: 100%;
		height: 60%;
	}
	
	.bottom {
		width: 100%;
		height: 20%;
		background-color: #A399C2;
	}
	
	.centerPanel {
		background-color: #ABCED4;
		height: 100%;
	}
	
	.leftPanel,.rightPanel {
		width: 50%;
		height: 100%;
		float: left;
	}
	
	.save {
		width: 80px;
		height: 30px;
		float: right;
		margin: 10px;
	}
	
	.gwt-TabLayoutPanel {
		min-height: 500px;
		background-color: #5A8A9E;
	}
	</ui:style>
	<g:HTMLPanel>
		<div class="{style.main}">
			<div class="{style.top}">

			</div>

			<div class="{style.middle}">
				<g:TabLayoutPanel barUnit="PX" barHeight="40"
					width="100%" height="100%" ui:field="panel">
				</g:TabLayoutPanel>
			</div>

			<div class="{style.bottom}">
				<div class="{style.save}">
					<g:Button text="Open" ui:field="save" />
				</div>
				<div class="{style.save}">
					<g:Button text="Cancel" ui:field="cancel" />
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 